<script>
$(document).ready(function()
{
	$('.editText').editable('/my.php',
	{
		maxlength : '48',
		tooltip : 'Click to edit...',
		id : 'fieldname',
        name : 'content',
		height : 18,
		width : 140,
		onblur : 'submit',
		select : true,
		submit : '&nbsp;ok&nbsp;'
	});
});

function rotate(angle,id,sid,code,v)
{
	$.post("/rotate.php",{'sid':encodeURI(sid),'code':encodeURI(code),'v':encodeURI(v),'angle':encodeURI(angle)},function(data)
	{
		if(data=='OK')
		{
			var date = new Date();
			
			var src = $("#id_"+id).attr('src');
			var pos = src.indexOf('?');
			
			if (pos >= 0)
			{
				src = src.substr(0, pos);
			}
			
			$("#id_"+id).attr("src",src + '?noCache=' + date.getTime());
			
			get_img_size("#id_"+id);
		}
	});
}

function get_img_size(id)
{
	var src=$(id).attr('src');
	
    var newImg = new Image();
    newImg.src = src;
	
    var height = newImg.height;
    var width = newImg.width;
	
    $(newImg).load(function()
	{
		$(id).width(newImg.width);
		$(id).height(newImg.height);
    });
}
</script>
<script type="text/javascript">new Image().src = "//counter.yadro.ru/hit?r" + escape(document.referrer) + ((typeof(screen)=="undefined")?"" : ";s"+screen.width+"*"+screen.height+"*" + (screen.colorDepth?screen.colorDepth:screen.pixelDepth)) + ";u"+escape(document.URL) + ";h"+escape(document.title.substring(0,80)) + ";" +Math.random();</script>
